fication(201, resp)\r\n\t\t\t\t\t\tthis.loadingSubmit = false\r\n\t\t\t\t\t} catch (error) {\r\n\t\t\t\t\t\tconsole.log(error)\r\n\t\t\t\t\t\tthis.loadingSubmit = false\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t} else {\r\n\t\t\t\tthis.validateChilds = !this.validateChilds\r\n\t\t\t\tdocument.getElementById('main').scrollTo(0, 0)\r\n\t\t\t}\r\n\t\t},\r\n\t\tvalidsOnEdit(value) {\r\n\t\t\treturn this.$route.params.id ? false : value\r\n\t\t}\r\n\t}\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components/views/ship"}]}